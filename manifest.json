{
  "name": "SQL Importer",
  "description": "Import SQL data from your own database",
  "picture": "picture.png",
  "version": "0.0.1",
  "tags": [
    "incoming",
    "oneColumn"
  ],
  "private_settings": [
    {
      "name": "enabled",
      "title": "Enable Sync",
      "type": "boolean",
      "default": "false"
    },
    {
      "name": "db_type",
      "type": "string",
      "title": "Database Type",
      "enum": [
        "postgres",
        "mysql"
      ],
      "default": "postgres"
    },
    {
      "name": "connection_string",
      "title": "Connection String",
      "type": "string"
    },
    {
      "name": "query",
      "title": "Query to run",
      "type": "string",
      "format": "textarea"
    },
    {
      "name": "overwrite",
      "title": "Overwrite data in Hull in case of a conflict",
      "type": "boolean",
      "default": "true"
    },
    {
      "name": "last_sync_at",
      "title": "Last sync",
      "type": "string",
      "format": "hidden"
    },
    {
      "name": "last_updated_at",
      "title": "Last updated_at",
      "type": "string",
      "format": "hidden"
    },
    {
      "name": "last_job_id",
      "title": "Last job ID",
      "type": "string",
      "format": "hidden"
    }
  ],
  "resources": [],
  "readme": "readme.md",
  "admin": "admin.html",
  "ui": false,
  "schedules": [
    {
      "url": "/sync",
      "type": "cron",
      "value": "0 */3 * * *"
    }
  ]
}
